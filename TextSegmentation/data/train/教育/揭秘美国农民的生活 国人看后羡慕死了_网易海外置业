            "AnvTBwaeHn8nyEouAtJFsWhrJ8d2hZL5xEFbbGQomRt4wrIKIxaDznIuMmKST_KK"
        ];
	var randomIndex = Math.floor(Math.random() * keyList.length);
	
	document.getElementById("aticle_house_map1427699956543").src = "http://dev.virtualearth.net/REST/v1/Imagery/Map/Road/0.0,0.0/9?mapSize=215,111&format=jpeg&centerPoint=0.0,0.0&key="+keyList[randomIndex];
